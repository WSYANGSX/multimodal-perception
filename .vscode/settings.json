{
    "[python]": {
        "editor.rulers": [
            120
        ], // Python 文件显示 80 字符红线
        "editor.wordWrapColumn": 120
    },
    "flake8.args": ["--max-line-length=120"],
    "python.terminal.executeInFileDir": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/src"
    ],
}